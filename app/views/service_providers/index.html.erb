
 <table class="table table-hover table-condensed"> <thead>
      <tr>
      	
        <th>Name</th>
        <th>Status</th>
        <th>Contact Number</th>
        <th>Address</th>
        <th>Created At</th>
      </tr>
        </thead>
        <tbody> 
        <% @service_providers.each do |post| %>   
       <tr>
         <td><%= link_to post.name, post%></td>
         <td><%=post.status%></td>
         <td><%=post.contactno%></td>
         <td><%=post.address%></td>
         <td><%= post.created_at.strftime("%b %d. %Y")%></td>
      </tr><% end %></tbody></table>
</br></br>
<p><%= link_to "Add New", new_service_provider_path %></p>