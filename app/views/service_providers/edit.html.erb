 <div class="col-md-6 col-md-offset-3">
    <%= form_for(@service_provider) do |f| %>
     
      <%= f.label :name %> 
      <%= f.text_field :name, class: 'form-control' %>
       </br> </br>
      <%= f.label :status %>
      <%= f.select :status, ['Active', 'Inactive'] %>
          </br> </br>
      <%= f.label :description %>
      <%= f.text_field :description, class: 'form-control' %>
       </br> </br>
      <%= f.label :contact_no %>
      <%= f.text_field :contactno, class: 'form-control' %>
           </br> </br>
      <%= f.label :address %>
      <%= f.text_field :address, class: 'form-control' %>
         </br> </br>
      <%= f.submit "Create account", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
   <% if @service_provider.errors.any? %>
  <div>
    <div>
      The form contains <%= pluralize(@service_provider.errors.count, "error") %>.
    </div>
    <ul>
    <% @service_provider.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
</div>