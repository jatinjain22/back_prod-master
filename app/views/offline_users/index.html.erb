<p id="notice"><%= notice %></p>

<h1>Listing Offline Users</h1>

<table class="table table-hover table-condensed">
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Contactno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offline_users.each do |offline_user| %>
      <tr>
        <td><%= offline_user.name %></td>
        <td><%= offline_user.address %></td>
        <td><%= offline_user.contactno %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add New User', new_offline_user_path %>
