<p id="notice"><%= notice %></p>

<h1>Listing Recurrings</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Recurring flag</th>
      <th>Last date</th>
      <th></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recurrings.each do |recurring| %>
      <tr>
        <td><%= recurring.user_id %></td>
        <td><%= recurring.recurring_flag %></td>
        <td><%= recurring.last_date %></td>
        <td><%= recurring. %></td>
        <td><%= link_to 'Show', recurring %></td>
        <td><%= link_to 'Edit', edit_recurring_path(recurring) %></td>
        <td><%= link_to 'Destroy', recurring, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Recurring', new_recurring_path %>
